{
  "apiVersion": 3,
  "name": "carpentry/hero",
  "version": "0.1.0",
  "title": "Hero Section",
  "category": "carpentry-blocks",
  "icon": "cover-image",
  "description": "Hero section con título, descripción y call-to-action",
  "supports": {
    "html": false,
    "align": ["wide", "full"]
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Expertos <br>en Reformas"
    },
    "description": {
      "type": "string",
      "default": "Nos enfocamos en entender las necesidades y deseos de nuestros clientes para ofrecerles resultados que superen sus expectativas."
    },
    "backgroundImage": {
      "type": "string",
      "default": ""
    },
    "buttonText": {
      "type": "string",
      "default": "Ver más"
    },
    "buttonUrl": {
      "type": "string",
      "default": "#servicios"
    }
  },
  "textdomain": "carpentry-blocks",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}
