{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "carpentry/footer",
  "title": "Carpentry Footer",
  "description": "Footer profesional para ServiLucas con informaci√≥n de contacto y enlaces",
  "category": "theme",
  "icon": "admin-appearance",
  "keywords": [
    "footer",
    "contact",
    "carpentry",
    "servilucas"
  ],
  "attributes": {
    "logoId": {
      "type": "number",
      "default": 0
    },
    "logoUrl": {
      "type": "string",
      "default": ""
    },
    "logoAlt": {
      "type": "string",
      "default": ""
    },
    "useGlobalFooterLogo": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "html": false,
    "reusable": true
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}